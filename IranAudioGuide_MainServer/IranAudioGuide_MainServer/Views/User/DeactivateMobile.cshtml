@{
    Layout = null;
}
@using IranAudioGuide_MainServer.App_GlobalResources


<div class="row">

    <nav class="subheader">
        <div class="nav-wrapper subheader black-text">
            <div class="col s12">
                <h4 style="display:inline-block"> @Global.MenuDevices</h4>
            </div>
        </div>
    </nav>


    <div class="col s12">

        <div class="alert  alert-success" ng-show="m.isShowMessage">
            <strong> Success!</strong> {{m.msg}}
        </div>

        <div class="card IAGblueBack darken-1">
            <div class="card-content white-text">
                <span class="card-title">@Global.PageProfileDevicesWantTo</span>
                <ul>
                    <li> @Global.PageProfileByPressingThe <b> @Global.PageProfileDeactive</b> @Global.PageProfileDeactiveMsg</li>
                    <li> @Global.PageProfileDeactiveMsg2</li>
                    <li> <b>@Global.Remmember !</b> @Global.PageProfileDeactiveMsg3</li>
                </ul>
            </div>
            <div class="card-action">
                <p class="notSetDevice"> @Global.PageProfileDeactiveYouHaveNotSet </p>
                <button type="button" ng-disabled="!user.IsAccessChangeUuid"
                        ng-show="user.IsSetuuid"
                        class="waves-effect waves-light btn z-depth-1"
                        ng-click="deactivateMobile()">
                    @Global.DeactivateOldDevice
                </button>
                
            </div>
        </div>
    </div>

</div>




