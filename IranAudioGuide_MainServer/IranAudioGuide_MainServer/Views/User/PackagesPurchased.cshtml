
@{
    Layout = null;
}


<div class="PlanPricing template4" ng-repeat="pak in packagesPurchased" ng-class="{'col-flex-1':pak.PackageCities.length == 1, 'col-flex-2':pak.PackageCities.length > 1}">
    <!-- Price template4 Starts -->
    <div class="planName">
        @*<span class="price">{{pak.PackagePrice}}IRR </span>*@
        <h3>{{pak.PackageName}}</h3>
        @*<p>{{pak.PackageName}}</p>*@
    </div>
    <div class="planFeatures">
        <!-- Slideshow 4 -->
        <div class="callbacks_container">
            <ul class="slider6 rslides">
                <li ng-repeat="city in pak.PackageCities">

                    <img ng-src="{{city.CityImageUrl}}" />
                    <p class="cityName">{{city.CityName}}</p>
                    <p class="caption">{{city.CityDesc}}</p>
                    <ul>
                        <li ng-repeat="plc in city.Places">{{plc.PlaceName}}</li>
                    </ul>
                </li>
            </ul>
        </div>

    </div>
   
    @*<button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#myModal">Open Modal</button>*@
</div>


<div ng-show="isShowMessage == true" style="padding:10%">
    @*<h4 class="block"> Packages Purchased</h4>*@
    
    <p style="color:#001644"><i class="fa fa-info-circle"></i>&nbsp; @Resources.Global.PageProfile25</p>
</div>
